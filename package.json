{
  "name": "console-shared-types",
  "version": "1.0.4",
  "description": "Shared TypeScript types and classes for console applications",
  "main": "build/index.js",
  "files": [
    "/build"
  ],
  "types": "build/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:script": "npm run build:cjs && npm run build:esm && npm run fix:esm",
    "build:cjs": "tsc",
    "build:esm": "tsc -p tsconfig.esm.json",
    "fix:esm": "node scripts/fix-esm-imports.js",
    "dev": "npm run build && npm run dev:watch",
    "dev:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "typescript",
    "types",
    "console",
    "shared"
  ],
  "author": "Jackmay Confidence",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Finsel-DGI/console-shared-types.git"
  },
  "bugs": {
    "url": "https://github.com/Finsel-DGI/console-shared-types/issues"
  },
  "homepage": "https://github.com/Finsel-DGI/console-shared-types#readme",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.0.0"
  }
} 